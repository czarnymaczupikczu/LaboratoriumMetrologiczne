<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>

<VBox maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="400.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.userViewController">
   <children>
      <TableView fx:id="userTableView" prefHeight="443.0" prefWidth="800.0">
        <columns>
          <TableColumn fx:id="idUserColumn" maxWidth="50.0" minWidth="50.0" prefWidth="50.0" text="Lp." />
          <TableColumn fx:id="firstNameColumn" prefWidth="75.0" text="Imię" />
            <TableColumn fx:id="lastNameColumn" prefWidth="75.0" text="Nazwisko" />
            <TableColumn fx:id="loginColumn" prefWidth="75.0" text="Login" />
            <TableColumn fx:id="passwordColumn" prefWidth="75.0" text="Hasło" />
            <TableColumn fx:id="permissionLevelColumn" prefWidth="75.0" text="Uprawnienia" />
        </columns>
         <columnResizePolicy>
            <TableView fx:constant="CONSTRAINED_RESIZE_POLICY" />
         </columnResizePolicy>
      </TableView>
      <HBox alignment="CENTER" prefHeight="100.0" prefWidth="200.0">
         <children>
            <Button fx:id="addUserButton" mnemonicParsing="false" onAction="#addUser" prefHeight="50.0" prefWidth="100.0" text="Nowy">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
            <Button fx:id="editUserButton" mnemonicParsing="false" onAction="#editUser" prefHeight="50.0" prefWidth="100.0" text="Edytuj">
               <HBox.margin>
                  <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
               </HBox.margin>
            </Button>
         </children>
      </HBox>
   </children>
</VBox>
