                        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
                    </HBox.margin>
                </Button>
                <Button fx:id="cancelSaveUserButton" mnemonicParsing="false" onAction="#cancelSaveUser" prefHeight="50.0" prefWidth="100.0" text="Anuluj">
